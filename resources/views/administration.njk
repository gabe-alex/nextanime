
{# resources/views/administration.html #}

{% extends 'layout' %}

{% block content %}

  {# action="/create?_method=PUT"#}
  <br>
  <div style="padding: 0 15px">
    <div class="col-xs-12 col-md-6">
      <form method="post" role="form" enctype="multipart/form-data"> {#enctype="multipart/form-data" is for encoding the upload file, to allow it to be uploaded#}
        {{ csrfField | safe }}
        <label for="upload_database">Upload Database:</label>
        <input type="file" class="btn btn-primary" name="database_file"/>
        <br>
        <input type="submit" class="btn btn-primary" value="Upload Database"/>
      </form>
    </div>
    <div class="col-xs-12 col-md-6">

    </div>
    <br>

  </div>

{% endblock %}
